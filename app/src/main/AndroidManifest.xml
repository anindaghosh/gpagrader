<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.khakidonald.srmgrader">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_foreground"
        android:supportsRtl="true"
        android:theme="@style/AppThemeMat.NavMenu">
        <activity android:name=".cgpaResult">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.khakidonald.srmgrader.cgpaCalulate" />
        </activity>
        <activity android:name=".sgpaResult">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.khakidonald.srmgrader.MainActivity" />
        </activity>
        <activity
            android:name=".cgpaCalulate"
            android:label="@string/title_activity_cgpa_calulate"></activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>